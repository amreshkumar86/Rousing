.jqvmap-label
  position: absolute
  display: none
  border-radius: 2px
  background: #292929
  color: white
  font-family: sans-serif, Verdana
  font-size: smaller
  pointer-events:none
  padding: 0.5em
  line-height: 1
  z-index: $over-all

.jqvmap-pin
  pointer-events:none

.jqvmap-zoomin, .jqvmap-zoomout
  position: absolute
  left: 1em
  border-radius: 2px
  background: $color-variant-primary
  color: white
  width: 2em
  height: 2em
  cursor: pointer
  line-height: 2em
  text-align: center

.jqvmap-zoomin
  top: 1em

.jqvmap-zoomout
  top: 3.2em

.jqvmap-region
  cursor: pointer

.jqvmap-ajax_response
  width: 100%
  height: 500px




.c-map
  width: 100%
  height: 100%
  position: relative
