.flotTip
  z-index: 100
  padding: 0.5em 1em
  font-size: 0.875em
  color: $color-main
  background-color: $color-grey-pivotal
  border: 0
  border-radius: 2px

.legendColorBox,
.legendLabel
  padding: 0
  color: $color-grey-minor


.legendColorBox > div
  border: none !important
  margin: 0.5em

.legendColorBox > div > div
    border-radius: 10px

.legend > div:first-child
  display: none
