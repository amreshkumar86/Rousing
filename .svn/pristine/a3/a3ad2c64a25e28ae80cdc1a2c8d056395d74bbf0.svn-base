;(function () {

  'use strict';


  /**
   * ======================
   * Registering Controller
   * ======================
   */

  angular
    .module( 'ponut.blank' )
    .controller( 'blankCtrl', blankCtrl );


  /**
   * ===================
   * Dependacy Injection
   * ===================
   */

  blankCtrl.$inject = [ '$rootScope' ];


  /**
   * =========================
   * Controller Function Setup
   * =========================
   *
   * [1] : Defining page title.
   *
   */

  function blankCtrl( $rootScope ) {
    // [1]
    $rootScope.title = 'Blank';

    var vm = this;
  }

})();
